{
    "problemMatcher": [
        {
            "owner": "rustlang",
            "pattern": [
                {
                    "regexp": "^----\\s(.+)\\sstdout\\s----$",
                    "message": 1
                },
                {
                    "regexp": "^.*,\\s(\\S+):(\\d+):(\\d)$",
                    "file": 1,
                    "line": 2,
                    "column": 3
                }
            ]
        }
    ]
}