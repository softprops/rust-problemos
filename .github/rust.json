{
    "problemMatcher": [
        {
            "owner": "cargo-test",
            "pattern": [
                {
                    "regexp": "^---- (\\S+) stdout ----$"
                },
                {
                    "regexp": "^thread '\\S+' panicked at '(.+)', (\\S+):(\\d+):(\\d+)$",
                    "message": 1,
                    "file": 2,
                    "line": 3,
                    "column": 4
                }
            ]
        }
    ]
}